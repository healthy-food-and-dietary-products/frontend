{
	"extends": ["stylelint-config-standard", "stylelint-config-standard-scss"],
	"plugins": ["stylelint-scss"],
	"rules": {
		"selector-class-pattern": null
	},
	"processors": ["stylelint-scss"],
	"ignoreFiles": [
		"build/**",
		"coverage/**",
		"dist/**",
		"**/*.js",
		"**/*.jsx",
		"**/*.ts",
		"**/*.tsx"
	]
}
